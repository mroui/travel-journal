    <string name="create_travel_title">Create travel</string>
    <string name="create_travel_stage_0_title">Hey Traveler!</string>
    <string name="create_travel_stage_0_desc">Looks like you want to create a new journey.
        That\'s awesome! This creator will guide you
        step by step.\n\nLet\'s start from the beginning…
    </string>
    <string name="create_travel_stage_1_title">Name</string>
    <string name="create_travel_stage_1_desc">Choose it wisely,\nit will identify your
        journey in the future.
    </string>
    <string name="create_travel_stage_1_name_hint">Awesome 3 days trip in Mediolan</string>
    <string name="create_travel_stage_2_title">Image</string>
    <string name="create_travel_stage_2_desc">Upload picture which could also identify your journey.
        It\'s <b>optional</b> but better to have one than not, right?
    </string>
    <string name="create_travel_stage_2_upload_image">Upload image</string>
    <string name="create_travel_stage_2_upload_image_file_remove">Remove image</string>
    <string name="create_travel_stage_3_title">Date and time</string>
    <string name="create_travel_stage_3_desc_1">First of all, choose departure
        date.\nIt\'s also good to plan approximate time, for example time on travel ticket
    </string>
    <string name="create_travel_stage_3_from_title">FROM</string>
    <string name="create_travel_stage_3_to_title">TO</string>
    <string name="create_travel_stage_3_desc_2">… and similar to arrival date and time.</string>
    <string name="create_travel_stage_3_set_alarm_title">Set alarm to departure time</string>
    <string name="create_travel_stage_4_title">Destination</string>
    <string name="create_travel_stage_4_desc">Where are you going? Country, city, any other
        specific place?
    </string>
    <string name="create_travel_stage_5_title">Transport</string>
    <string name="create_travel_stage_5_desc">Select the type of transport,\nit will appear in the
        travel tags.
    </string>
    <string name="create_travel_stage_5_transport_upload_file">Upload file</string>
    <string name="create_travel_stage_5_transport_upload_file_desc"><b>Optionally,</b> you can
        upload important file, such as ticket to keep it around.
        \nAvailable type: image/pdf
    </string>
    <string name="create_travel_stage_5_transport_contact_desc">Also <b>optionally</b>, it\'s good
        to have a contact, for example to carrier.
    </string>
    <string name="create_travel_stage_5_transport_contact_hint">Phone number, e-mail address…
    </string>
    <string name="create_travel_stage_5_file_remove">Remove file</string>
    <string name="create_travel_stage_6_title">Accommodation</string>
    <string name="create_travel_stage_6_desc">Select the type of accommodation,\nit will appear in
        the travel tags.
    </string>
    <string name="create_travel_stage_6_accommodation_upload_file_desc"><b>Optionally,</b> you can
        upload important file for booking to keep it around.
    </string>
    <string name="create_travel_stage_6_accommodation_upload_file">Upload file</string>
    <string name="create_travel_stage_6_accommodation_contact_desc">Also <b>optionally</b>, it\'s good
        to have some form of contact.
    </string>
    <string name="create_travel_stage_6_accommodation_contact_hint">Phone number, e-mail address…
    </string>
    <string name="create_travel_stage_6_file_remove">Remove file</string>
    <string name="create_travel_stage_7_title">Budget</string>
    <string name="create_travel_stage_7_desc">Enter total estimated budget for the trip including
        accommodation, transport, meals, expenses… all of them.
    </string>
    <string name="create_travel_stage_8_title">Tags</string>
    <string name="create_travel_stage_8_desc_1">Enter and choose tags that best suit your journey.
        You can enter places, types, activities and more.
    </string>
    <string name="create_travel_stage_8_desc_2">Added tags will characterize your trip.</string>
    <string name="create_travel_stage_8_tags_hint">Beach, europe, culture, landscape…</string>
